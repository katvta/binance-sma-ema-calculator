# URL base da API da Binance (produção)
API_URL=https://api.binance.com/api

# Par de moedas monitorado (ex: Bitcoin/USDT)
SYMBOL=BTCUSDT

# Intervalo das velas (ex: 1h, 4h, 1d)
INTERVAL=1h

# Período para cálculo das médias (ex: 20 períodos)
PERIOD=20

# Intervalo de atualização em milissegundos (ex: 3000ms = 3s)
REFRESH_INTERVAL=3000